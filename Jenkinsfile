pipeline{
  agent any
  
  stages{
    stage('hello2'){
      steps{
           echo 'Hello India'
      }
    }
    
    
    stage('Build1'){
      steps{
           echo 'Building1'
      }
    }
    
    stage('Deploy2'){
      steps{
           echo 'Deploying'
      }
    }
    
    stage('Test1'){
      steps{
           echo 'Testing'
      }
    }
    
    stage('Release'){
      steps{
           echo 'Releasing'
      }
    }
  }
}
